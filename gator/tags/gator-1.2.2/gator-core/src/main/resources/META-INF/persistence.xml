<?xml version="1.0" encoding="UTF-8"?>
<persistence
  xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0">
  <persistence-unit name="gator" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>edu.vt.middleware.gator.AppenderConfig</class>
    <class>edu.vt.middleware.gator.AppenderParamConfig</class>
    <class>edu.vt.middleware.gator.CategoryConfig</class>
    <class>edu.vt.middleware.gator.ClientConfig</class>
    <class>edu.vt.middleware.gator.LayoutParamConfig</class>
    <class>edu.vt.middleware.gator.ParamConfig</class>
    <class>edu.vt.middleware.gator.PermissionConfig</class>
    <class>edu.vt.middleware.gator.ProjectConfig</class>
    <exclude-unlisted-classes />
    <validation-mode>NONE</validation-mode>
  </persistence-unit>
</persistence>
